{
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "usage",
        "corejs": "3.26.1"
      }
    ],
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    // "@liutsing/babel-plugin-react-data-testid",
    "react-refresh/babel",
    [
      "@liutsing/babel-plugin-remove-console",
      {
        "exclude": ["debug", "error", "warn"]
      }
    ]
    // [
    //   "import",
    //   {
    //     "libraryName": "lodash",
    //     "libraryDirectory": "",
    //     "camel2DashComponentName": false // default: true
    //   }
    // ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": true
            },
            "modules": "commonjs"
          }
        ]
      ]
    }
  }
}
