{
  "name": "@liutsing/webpack-react",
  "private": true,
  "version": "1.0.0",
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.prod.js",
    "serve": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js",
    "lint": "npm run lint",
    "lint:debug": "npm run lint --debug",
    "test": "npm run test",
    "test:watch": "npm run test --watch",
    "lint:fix": "npm run lint --fix"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.23.2",
    "antd": "^5.19.2",
    "core-js": "^3.33.1",
    "dayjs": "^1.11.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@liutsing/babel-plugin-remove-console": "0.0.1",
    "@liutsing/eslint-config": "0.0.11",
    "@liutsing/html-webpack-inject-plugin": "workspace:*",
    "@liutsing/prettier-config": "0.0.6",
    "@liutsing/tsconfig": "0.0.6",
    "@liutsing/types-utils": "workspace:*",
    "@liutsing/webpack-config": "workspace:*",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/react": "18.2.0",
    "@types/react-dom": "18.2.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "css-modules-typescript-loader": "^4.0.1",
    "mini-css-extract-plugin": "^2.9.0",
    "postcss-loader": "^8.1.1",
    "react-refresh": "0.14.0",
    "style-loader": "^4.0.0",
    "svgo-loader": "^4.0.0",
    "tailwindcss": "^3.3.4",
    "thread-loader": "^4.0.2",
    "typescript": "^5.0.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "prettier": "@liutsing/prettier-config",
  "simple-git-hooks": {
    "pre-commit": "pnpx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "browserslist": [
    "last 2 versions",
    ">0.1%"
  ]
}
