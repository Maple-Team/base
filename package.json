{
  "name": "pangu",
  "version": "0",
  "description": "MapleImage tools base repo",
  "keywords": [
    "prettier规范",
    "eslint规范",
    "cli"
  ],
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "bootstrap": "pnpm i --registry=https://registry.npmmirror.com",
    "build:all": "nx run-many --target=build --all  --exclude example-1 example-2 example-3 example-4",
    "patch:all": "nx run-many --target=new:patch --all",
    "publish:all": "pnpm -r publish",
    "graph": "nx graph",
    "lint": "eslint ./packages --ext=.js --ext=.ts",
    "lint2": "eslint --ext .js,.ts,.tsx,.vue --fix ./packages/*/src",
    "prettier": "prettier --write --ignore-unknown '**/*'",
    "tsc": "nx run-many --target=tsc --all"
  },
  "prettier": "@liutsing/prettier-config",
  "dependencies": {
    "core-js": "^3.26.1",
    "dayjs": "^1.11.6"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-runtime": "^7.19.6",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/runtime": "^7.20.6",
    "@liutsing/eslint-config": "workspace:*",
    "@liutsing/prettier-config": "workspace:*",
    "@liutsing/regexp-utils": "workspace:*",
    "@liutsing/tsconfig": "^0.0.4",
    "@liutsing/types-utils": "workspace:*",
    "@liutsing/utils": "workspace:*",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "@types/jest": "^28.1.1",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.7",
    "babel-jest": "^28.1.1",
    "babel-loader": "^9.1.0",
    "benny": "^3.7.1",
    "css-loader": "^6.7.2",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "gen-esm-wrapper": "^1.1.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "lint-staged": "^13.0.3",
    "microbundle": "^0.15.1",
    "nx": "^14.7.5",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^3.4.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^28.0.5",
    "ts-node": "^10.8.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.4",
    "vue-tsc": "^1.0.10",
    "webpack": "^5.75.0"
  },
  "authors": [
    "liutsing <liutsingluo@gmail.com>"
  ]
}