{
  "name": "@liutsing/utils",
  "version": "0.4.0",
  "description": "",
  "sideEffects": false,
  "keywords": [],
  "license": "ISC",
  "author": "",
  "main": "lib/index.js",
  "module": "es/index.js",
  "source": "src/index.ts",
  "jsnext:main": "es/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib",
    "es",
    "types"
  ],
  "exports": {
    "import": "./es/index.js",
    "require": "./lib/index.js",
    "default": "./es/index.js",
    "types": "./types/index.d.ts"
  },
  "scripts": {
    "prebuild": "npx rimraf es lib types",
    "prepublish": "pnpm run build",
    "build:watch": "tsc -w",
    "build": "npm run build:es & npm run build:cjs",
    "postbuild": "npm run build:types",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel src -d lib --extensions .ts --delete-dir-on-start",
    "build:es": "babel src -d es --extensions .ts --delete-dir-on-start",
    "build:types": "tsc --project tsconfig.build.json",
    "test": "nx test",
    "test:watch": "nx test --watch",
    "new:patch": "npm version patch",
    "postpublish1": "pnpm run doc",
    "postpublish": "pnpx @liutsing/npmmirror-sync",
    "doc": "typedoc",
    "postdoc": "rimraf ../../docs/utils && mv docs ../../docs/utils"
  },
  "prettier": "@liutsing/prettier-config",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/helper-plugin-utils": "^7.22.5",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.0",
    "@babel/runtime": "^7.23.1",
    "@types/js-md5": "^0.7.0",
    "cross-env": "^7.0.3"
  },
  "dependencies": {
    "core-js": "^3.37.1",
    "@babel/runtime-corejs3": "^7.25.0",
    "@liutsing/regexp-utils": "workspace:*",
    "@liutsing/types-utils": "workspace:*",
    "js-md5": "^0.7.3"
  },
  "peerDependencies": {
    "lodash-es": "^4.17.21",
    "js-md5": "^0.7.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "nx": {
    "includedScripts": []
  }
}
