{
  "name": "@liutsing/html-webpack-plugin",
  "version": "0.3.0",
  "description": "A plugin to inject addition tag to supplement html-webpack-plugin",
  "keywords": [
    "webpack",
    "webpack plugin",
    "html-webpack-plugin"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "author": "liutsing <liutsingluo@gmail.com>",
  "exports": {
    "require": "./lib/index.js",
    "types": "./types/index.d.ts"
  },
  "main": "lib/index.js",
  "typings": "types/index.d.ts",
  "files": [
    "lib","types"
  ],
  "scripts": {
    "lint": "nx lint --ext .ts",
    "prettier": "prettier . --ext .ts -w",
    "prebuild": "rimraf lib types",
    "prepublish": "pnpm run build:babel",
    "test": "nx test",
    "postpublish": "pnpx @liutsing/npmmirror-sync",
    "build": "rollup -c rollup.config.js",
    "build:watch": "tsc -w",
    "build:babel": "npm run build:commonjs",
    "postbuild:babel": "npm run build:types",
    "build:commonjs": "babel src -d lib --extensions .ts --delete-dir-on-start",
    "build:types": "tsc --project tsconfig.build.json"
  },
  "prettier": "@liutsing/prettier-config",
  "dependencies": {
    "html-webpack-plugin": "^5.6.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@rollup/plugin-typescript": "^9.0.1",
    "rollup": "^4.17.2",
    "webpack": "^5.75.0"
  },
  "nx": {
    "includedScripts": []
  }
}
